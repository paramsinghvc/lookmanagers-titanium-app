<Alloy>
	<Collection src="feeds"/>
	<Tab title="Feeds" icon="images/feed.png" id="feedsTab">
		<Window title="LookManagers">
			<TableView name="feedsTable" id="feedsList" dataCollection="feeds" onClick="feedsRowClick">
				<TableViewRow class="feedsRow">					
					<View class="fHolder">
						<Label id="name" text="{text}" top="5" class="feedText" />						
					</View>
				</TableViewRow>
			</TableView>
		</Window>
	</Tab>
</Alloy>